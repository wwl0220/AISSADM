<security-function-requirement-policy>
    <policy-name>detect-DDoS</policy-name>
    <security-function-set>
        <security-function>
            <security-function-name>firewall</security-function-name>
            <security-function-property>security-component</security-function-property>
            <computing-resource>1.0</computing-resource>
        </security-function>
        <security-function>
            <security-function-name>network-ddos-attack-detection-module</security-function-name>
            <security-function-property>ai-security-service</security-function-property>
            <maximum-response-delay>60.0</maximum-response-delay>
        </security-function>
        <security-function>
            <security-function-name>application-ddos-attack-detection-module</security-function-name>
            <security-function-property>ai-security-service</security-function-property>
            <maximum-response-delay>60.0</maximum-response-delay>
        </security-function>
        <security-function>
            <security-function-name>drdos-attack-detection-module</security-function-name>
            <security-function-property>ai-security-service</security-function-property>
            <maximum-response-delay>60.0</maximum-response-delay>
        </security-function>
        <security-function>
            <security-function-name>lddos-attack-detection-module</security-function-name>
            <security-function-property>ai-security-service</security-function-property>
            <maximum-response-delay>60.0</maximum-response-delay>
        </security-function>
        <security-function>
            <security-function-name>botnet-detection-module</security-function-name>
            <security-function-property>ai-security-service</security-function-property>
            <maximum-response-delay>60.0</maximum-response-delay>
        </security-function>
    </security-function-set>
    <ai-workflow-set>
        <ai-workflow>
            <ai-workflow-name>network-ddos-attack-detection-module</ai-workflow-name>
            <dataset-name>network-ddos-attack-detection-train-data.csv</dataset-name>
        </ai-workflow>
        <ai-workflow>
            <ai-workflow-name>lddos-attack-detection-module</ai-workflow-name>
            <dataset-name>lddos-attack-detection-train-data.csv</dataset-name>
        </ai-workflow>
    </ai-workflow-set>
    <security-service-function-chain>
        <branch-path>
            <branch-path-number>0</branch-path-number>
            <tcp>tcp</tcp>
            <branching-security-function-chain>
                <branching-security-function>
                    <branching-security-function-name>firewall</branching-security-function-name>
                </branching-security-function>
                <branching-security-function>
                    <branching-security-function-name>application-ddos-attack-detection-module</branching-security-function-name>
                </branching-security-function>
                <branching-security-function>
                    <branching-security-function-name>lddos-attack-detection-module</branching-security-function-name>
                </branching-security-function>
                <branching-security-function>
                    <branching-security-function-name>botnet-detection-module</branching-security-function-name>
                </branching-security-function>
                <branching-security-function>
                    <branching-security-function-name>network-ddos-attack-detection-module</branching-security-function-name>
                </branching-security-function>
            </branching-security-function-chain>
        </branch-path>
        <branch-path>
            <branch-path-number>1</branch-path-number>
            <udp>udp</udp>
            <branching-security-function-chain>
                <branching-security-function>
                    <branching-security-function-name>firewall</branching-security-function-name>
                </branching-security-function>
                <branching-security-function>
                    <branching-security-function-name>drdos-attack-detection-module</branching-security-function-name>
                </branching-security-function>
                <branching-security-function>
                    <branching-security-function-name>network-ddos-attack-detection-module</branching-security-function-name>
                </branching-security-function>
            </branching-security-function-chain>
        </branch-path>
    </security-service-function-chain>
    <virtual-link-set>
        <virtual-link>
            <virtual-link-number>0</virtual-link-number>
            <start-function-name>firewall</start-function-name>
            <end-function-name>application-ddos-attack-detection-module</end-function-name>
        </virtual-link>
        <virtual-link>
            <virtual-link-number>1</virtual-link-number>
            <start-function-name>application-ddos-attack-detection-module</start-function-name>
            <end-function-name>lddos-attack-detection-module</end-function-name>
        </virtual-link>
        <virtual-link>
            <virtual-link-number>2</virtual-link-number>
            <start-function-name>lddos-attack-detection-module</start-function-name>
            <end-function-name>botnet-detection-module</end-function-name>
        </virtual-link>
        <virtual-link>
            <virtual-link-number>3</virtual-link-number>
            <start-function-name>botnet-detection-module</start-function-name>
            <end-function-name>network-ddos-attack-detection-module</end-function-name>
        </virtual-link>
        <virtual-link>
            <virtual-link-number>4</virtual-link-number>
            <start-function-name>firewall</start-function-name>
            <end-function-name>drdos-attack-detection-module</end-function-name>
        </virtual-link>
        <virtual-link>
            <virtual-link-number>5</virtual-link-number>
            <start-function-name>drdos-attack-detection-module</start-function-name>
            <end-function-name>network-ddos-attack-detection-module</end-function-name>
        </virtual-link>
    </virtual-link-set>
</security-function-requirement-policy>
